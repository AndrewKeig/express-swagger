{
  "apiVersion": "1.0",
  "swaggerVersion": "1.2.5",
  "basePath": "http://127.0.0.1:8000",
  "apis": [
    {
      "path": "/authenticate",
      "description": "Login a user [-description]",
      "operations": [
        {
          "httpMethod": "POST",
          "summary": "Login user [-summary]",
          "notes": "Login a user",
          "responseClass": "Auth",
          "nickname": "login",
          "consumes": [
            "application/json"
          ],
          "produces": [
            "application/json",
            "application/xml"
          ],
          "protocols": [
            "http"
          ],
          "authorizations": [
            "authByOAuth"
          ],
          "parameters": [
            {
              "name": "Credentials",
              "description": "provide a username & password",
              "paramType": "body",
              "required": true,
              "dataType": "Credentials"
            },
            {
              "name": "appid",
              "description": "provide an application id",
              "paramType": "header",
              "required": true,
              "dataType": "String"
            }
          ],
          "errorResponses": [
            {
              "code": 401,
              "reason": "Unauthorised"
            },
            {
              "code": 400,
              "reason": "Bad request"
            },
            {
              "code": 500,
              "reason": "An internal server error"
            }
          ]
        }
      ]
    },
    {
      "path": "/logoff",
      "description": "Log a user out [-description]",
      "operations": [
        {
          "httpMethod": "POST",
          "summary": "Logoff a user [-summary]",
          "notes": "Logoff a user",
          "responseClass": "Auth",
          "nickname": "Logoff",
          "consumes": [
            "application/json"
          ],
          "produces": [
            "application/json",
            "application/xml"
          ],
          "protocols": [
            "http"
          ],
          "authorizations": [
            "authByOAuth"
          ],
          "parameters": [
            {
              "name": "token",
              "description": "provide a token",
              "paramType": "header",
              "required": true,
              "dataType": "String"
            },
            {
              "name": "appid",
              "description": "appid",
              "paramType": "header",
              "required": true,
              "dataType": "String"
            }
          ],
          "errorResponses": [
            {
              "code": 401,
              "reason": "Unauthorised user"
            },
            {
              "code": 400,
              "reason": "Bad request"
            },
            {
              "code": 500,
              "reason": "An internal server error"
            }
          ]
        }
      ]
    }
  ],
  "resourcePath": "/authenticate",
  "models": {
    "Auth": {
      "id": "Auth",
      "properties": {
        "token": {
          "type": "String"
        }
      }
    },
    "Credentials": {
      "id": "Credentials",
      "properties": {
        "username": {
          "type": "String",
          "required": true
        },
        "password": {
          "type": "String",
          "required": true
        }
      }
    }
  }
}